{
  "name": "react.typescript.webpack",
  "version": "1.0.0",
  "description": "Simplified, opinionated way to configure webpack for development and then build for production with hjs-webpack.",
  "scripts": {
    "start": "NODE_ENV=development hjs-dev-server",
    "build": "NODE_ENV=production webpack --config ./webpack.config.js",
    "deploy": "surge -p public -d react.studiomik.es",
    "commit": "git add --all && git commit -am \"$(date)\" && npm version minor && git push origin master --tags",
    "debug:webpack": "NODE_ENV=production node-nightly --inspect --debug-brk ./node_modules/.bin/webpack --config ./webpack.config.js",
    "debug:server": "NODE_ENV=development node-nightly --inspect --debug-brk ./node_modules/.bin/hjs-dev-server"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/miksansegundo/react.typescript.webpack.git"
  },
  "keywords": [
    "react",
    "typescript",
    "webpack"
  ],
  "author": "Mike San Segundo",
  "license": "ISC",
  "bugs": {
    "url": "https://gitlab.com/miksansegundo/react.typescript.webpack/issues"
  },
  "homepage": "https://gitlab.com/miksansegundo/react.typescript.webpack#README",
  "devDependencies": {
    "@types/axios": "^0.9.34",
    "@types/react": "^0.14.50",
    "@types/react-dom": "^0.14.19",
    "@types/react-redux": "^4.4.35",
    "ampersand-local-cache-mixin": "^3.0.2",
    "autoprefixer": "^6.5.3",
    "awesome-typescript-loader": "^3.0.0-beta.10",
    "axios": "^0.15.3",
    "child-compiler-loader-list-webpack-plugin": "^0.1.0",
    "classnames": "^2.2.5",
    "core-js": "^2.4.1",
    "css-loader": "^0.26.0",
    "cssnano": "^3.8.1",
    "enhanced-resolve": "^2.3.0",
    "file-loader": "^0.9.0",
    "hjs-webpack": "^8.4.2",
    "html-webpack-plugin": "^2.24.1",
    "isomorphic-style-loader": "^1.1.0",
    "json-loader": "^0.5.4",
    "material-design-icons": "^3.0.1",
    "moment": "^2.17.1",
    "normalize.css": "^5.0.0",
    "postcss-browser-reporter": "^0.5.0",
